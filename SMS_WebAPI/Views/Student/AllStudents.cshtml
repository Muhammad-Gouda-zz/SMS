<div ng-init="studentCtrl.getAllStudents()" >
    {{studentCtrl.helper.message}}
    <div>@Resources.SMSResource.SelectStudent</div>
    <div class="scrollable">
        <ul ng-repeat="student in studentCtrl.students">
            <li>{{student.FirstName}}</li>
        </ul>
    </div>
</div>
@Styles.Render("~/Content/SmsStyles/Controls.css")
